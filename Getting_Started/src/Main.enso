main =
    file1 = Data.read 'Data/Sales Targets and Results.xlsx'
    targets = file1.read 'Targets'
    results_2024 = file1.read '2024 Results'
    results_2025 = file1.read '2025 Results'
    table1 = targets.join results_2024 ..Inner [(..Equals 'Quarter' 'Quarter')]
    table3 = table1.filter (expr '[Sales]<[Target]') ..Is_False
    table4 = table3.select_columns ['Year', 'Quarter', 'Target', 'Sales'] True



